{
  "version": "0.2.0",
  "compounds": [
    {
      "name": "Full Stack: Server + Chrome",
      "configurations": [
        "Server: React Router Dev (Hono + Vite)",
        "Chrome: Debug Client (Vite)"
      ]
    }
  ],
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Server: React Router Dev (Hono + Vite)",
      "cwd": "/Users/huangjie/team-chat/app",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["dev"],
      "env": {
        "NODE_ENV": "development",
        "NODE_OPTIONS": "--inspect=9229 --enable-source-maps"
      },
      "envFile": "/Users/huangjie/team-chat/app/.env.dev",
      "console": "integratedTerminal",
      "autoAttachChildProcesses": true,
      "sourceMaps": true,
      "smartStep": true,
      "skipFiles": [
        "<node_internals>/**",
        "/Users/huangjie/team-chat/app/node_modules/**"
      ]
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Chrome: Debug Client (Vite)",
      "url": "http://localhost:5175/",
      "webRoot": "/Users/huangjie/team-chat/app/app",
      "sourceMaps": true,
      "smartStep": true
    }
  ]
}
